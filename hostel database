 12.MESS-MENU
 INSERT INTO `hostels`.`mess_menu` (`MESS_ID`) VALUES ('M-1'); 
INSERT INTO `hostels`.`mess_menu` (`MESS_ID`) VALUES ('M-2'); 
INSERT INTO `hostels`.`mess_menu` (`MESS_ID`) VALUES ('M-3'); 
 UPDATE `hostels`.`mess_menu` SET `M_DAY` = 'MON' WHERE `MESS_ID` = 'M-1' AND `M_DAY` = '' AND `M_TIME` = ''; 
UPDATE `hostels`.`mess_menu` SET `M_DAY` = 'MON' WHERE `MESS_ID` = 'M-2' AND `M_DAY` = '' AND `M_TIME` = ''; 
UPDATE `hostels`.`mess_menu` SET `M_DAY` = 'MON' WHERE `MESS_ID` = 'M-3' AND `M_DAY` = '' AND `M_TIME` = ''; 
 UPDATE `hostels`.`mess_menu` SET `M_TIME` = 'BREAKFAST' WHERE `MESS_ID` = 'M-1' AND `M_DAY` = 'MON' AND `M_TIME` = ''; 
 UPDATE `hostels`.`mess_menu` SET `M_TIME` = 'BREAKFAST' WHERE `MESS_ID` = 'M-2' AND `M_DAY` = 'MON' AND `M_TIME` = ''; 
 UPDATE `hostels`.`mess_menu` SET `M_TIME` = 'BREAKFAST' WHERE `MESS_ID` = 'M-3' AND `M_DAY` = 'MON' AND `M_TIME` = ''; 
UPDATE `hostels`.`mess_menu` SET `MENU` = 'IDLI, CHATNI' WHERE `MESS_ID` = 'M-1' AND `M_DAY` = 'MON' AND `M_TIME` = 'BREAKFAST'; 
 UPDATE `hostels`.`mess_menu` SET `MENU` = 'IDLI, CHATNI' WHERE `MESS_ID` = 'M-2' AND `M_DAY` = 'MON' AND `M_TIME` = 'BREAKFAST'; 
 UPDATE `hostels`.`mess_menu` SET `MENU` = 'IDLI, SAMBHAR' WHERE `MESS_ID` = 'M-3' AND `M_DAY` = 'MON' AND `M_TIME` = 'BREAKFAST'; 
UPDATE `hostels`.`mess_menu` SET `M_DAY` = 'TUES' WHERE `MESS_ID` = 'M-3' AND `M_DAY` = '' AND `M_TIME` = ''; 
 UPDATE `hostels`.`mess_menu` SET `M_DAY` = 'TUES' WHERE `MESS_ID` = 'M-3' AND `M_DAY` = '' AND `M_TIME` = ''; 
 UPDATE `hostels`.`mess_menu` SET `M_DAY` = 'WED' , `M_TIME` = 'BREAKFAST' WHERE `MESS_ID` = 'M-3' AND `M_DAY` = '' AND `M_TIME` = ''; 
UPDATE `hostels`.`mess_menu` SET `M_TIME` = 'BREAKFAST' , `MENU` = 'POHA' WHERE `MESS_ID` = 'M-3' AND `M_DAY` = 'TUES' AND `M_TIME` = ''; 
 UPDATE `hostels`.`mess_menu` SET `MENU` = 'PAV- BHAJI' WHERE `MESS_ID` = 'M-3' AND `M_DAY` = 'WED' AND `M_TIME` = 'BREAKFAST'; 
 INSERT INTO `hostels`.`mess_menu` (`MESS_ID`, `M_DAY`, `M_TIME`, `MENU`) VALUES ('M-3', 'THURS', 'BREAKFAST', 'PARATHA AALOO'); 
 INSERT INTO `hostels`.`mess_menu` (`MESS_ID`, `M_DAY`, `M_TIME`, `MENU`) VALUES ('M-3', 'SUN', 'BREAKFAST', 'AALO PARATHA');
